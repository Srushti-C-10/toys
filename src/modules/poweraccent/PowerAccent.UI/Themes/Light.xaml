<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=System.Runtime">

    <!--  Metadata  -->
    <system:String x:Key="Theme.Name">Light.Accent1</system:String>
    <system:String x:Key="Theme.Origin">PowerToysRun</system:String>
    <system:String x:Key="Theme.DisplayName">Accent1 (Light)</system:String>
    <system:String x:Key="Theme.BaseColorScheme">Light</system:String>
    <system:String x:Key="Theme.ColorScheme">Accent1</system:String>
    <Color x:Key="Theme.PrimaryAccentColor">White</Color>

    <SolidColorBrush x:Key="SecondaryBackgroundBrush" Color="#FFEEEEEE" />
    <SolidColorBrush x:Key="PrimaryBackgroundBrush" Color="#FFFFFFFF" Opacity="0.25" />
    <SolidColorBrush x:Key="WindowBorderBrush" Color="#FF757575" Opacity="0.4" />
    <SolidColorBrush x:Key="DividerBorderBrush" Color="#FF000000" Opacity="0.0578" />
    <SolidColorBrush x:Key="PrimaryForegroundBrush" Color="#FF000000" Opacity="0.8956" />
    <SolidColorBrush x:Key="SecondaryForegroundBrush" Color="#FF000000" Opacity="0.6189" />
</ResourceDictionary>