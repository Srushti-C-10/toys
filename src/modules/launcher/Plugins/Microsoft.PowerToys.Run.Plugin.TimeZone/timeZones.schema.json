{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "TimeZones": {
            "type": "array",
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Offset": {
                        "type": "string"
                    },
                    "Names": {
                        "type": "array",
                        "uniqueItems": true,
                        "items": {
                            "type": "string"
                        }
                    },
                    "Shortcut": {
                        "type": "string"
                    },
                    "DaylightSavingTime": {
                        "type": "boolean"
                    },
                    "Countries": {
                        "type": "array",
                        "uniqueItems": true,
                        "items": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    }
}
